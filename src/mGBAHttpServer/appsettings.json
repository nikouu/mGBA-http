{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore.StaticFiles.StaticFileMiddleware": "Error",
      "Microsoft.AspNetCore.Mvc.Infrastructure.DefaultActionDescriptorCollectionProvider": "Error",
      "Microsoft.AspNetCore.Hosting.Diagnostics": "Error"
    },
    "LogFilters": {
      "Microsoft": "Warning",
      "System": "Error"
    }
  },
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://localhost:5000"
      },
      "Https": {
        "Url": "https://localhost:5001"
      }
    }
  },
  "mgba-http": {
    "Socket": {
      "IpAddress": "127.0.0.1",
      "Port": 8888,
      "ReadTimeout": 3000,
      "WriteTimeout": 3000
    }
  },
  "AllowedHosts": "*"
}
